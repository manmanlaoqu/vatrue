/* pages/myconcerned_code/myconcerned_code.wxss */
page{
  background: #F1F4F9;
}
.scan_history{
  padding:16rpx 30rpx 32rpx;
  padding-right:0;
  background:#fff;
  padding-bottom:0;
   margin-left:30rpx;
   padding-left:0;
  
}

.border{
    border-top: 1rpx solid rgba(231,237,239,1); 
}
.scan{
  display: flex;
  padding-bottom:30rpx;
}

 .scan_history:first-child{
  /* border:none; */
} 
.one:first-child{
  border:2rpx solid red;
}
.time{
  padding:16rpx 30rpx;
   font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(69,81,90,1);
}
.scan_img image{
  margin-top:15rpx;
  border:1rpx solid #eee;
  width:240rpx;
  height:180rpx;
  margin-right:30rpx;
}
.scan_time{
  float:right;
  font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(134,144,163,1);
}
.detail{
  font-size:32rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(6,25,39,1);
}
.price{
  font-size:48rpx;
font-family:PingFangSC-Medium;
 font-weight:500; 
color:rgba(255,158,63,1);
}
.detail_wrap{
  padding-right:30rpx;
  flex:1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.price text{
  font-size:32rpx;
}
.detail{
  margin-bottom:12rpx;
}
.line{
  width:100%;
  height:1rpx;
  border-top:1rpx solid #eee;
}
.box{
  font-size:32rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(90,99,117,1);
}
.box_img image{
  width:75rpx;
  height:78rpx;
  position:absolute;
  top:0;
  right:30rpx;
}
.box_time{
  font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(134,144,163,1);
}
.more{
  font-size:32rpx;
  text-align: center;
  padding-top:40rpx;
  width:260rpx;
  height:70rpx;
  margin:0 auto
}
.more image{
  width:100%;
  height:100%;
}
.jumpTop{
  position:fixed;
  bottom:100rpx;
  right:20rpx;
  border-radius: 50rpx;
  height:80rpx;
  width:80rpx;
  border: 1rpx solid #eee;
}
.jumpTop image{
  width:100%;
  height:100%;
}
.jump{
  /* background: red; */
   display:block; 
}
.cang{
  display: none;
}